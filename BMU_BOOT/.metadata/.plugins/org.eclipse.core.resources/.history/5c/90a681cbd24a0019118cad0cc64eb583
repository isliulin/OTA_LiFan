
/***************************************************************************************/
/*UDS 程序控制定义*/
#define	SID_RC_DID_0203			0x0203U		/*编程预处理*/
#define	SID_RC_DID_FF00			0xFF00U		/*擦除Appl有效标志*/
#define SID_RC_DID_FF01         0xFF01U
#define SID_RC_DID_0202         0x0202U
#define SID_RC_DID_0200         0x0200U
#define SID_RC_DID_F518         0xF518U
#define SID_RC_DID_DEF0			0xDEF0

#define	STD_RC_DID_FF00_IDLE_STATE		0U	/*空闲状态*/
#define	STD_RC_DID_FF00_START_STATE		1U	/*擦除Appl有效标志启动状态*/
#define	STD_RC_DID_FF00_WAIT_STATE		2U	/*擦除Appl有效标志等待完成状态*/

#define STD_RC_BACKUPSTAR 0xA0020000
#define STD_RC_BACKUPSTOP 0xA007FF00
#define STD_RC_INTERRSTAR 0xA00C0000
#define STD_RC_INTERRSTOP 0xA00FFF00

typedef	struct
{
	uint8	ui8_STR_Flag;				/*startRoutine标志*/
	uint8	ui8_DID_FF00_Task_FSM;		/*DID FF00状态机*/
	boolean	b_PreProgrammingFlag;		/*预刷新标志*/
	boolean b_BackUpFlag;				/*备份标志*/
	boolean b_UpdateFlag;				/*升级标志*/
}Struct_Uds_SID_RC_Info;		/*UDS SID_RC信息*/
extern	Struct_Uds_SID_RC_Info	st_Uds_SID_RC_Info;	/*UDS SID_RC信息*/

/*************************************************************************/
/*
Function:		void UdsFunc_SID_RC_SUBID_STR_Callback
				(uint8 *ui8_pInd, uint8 *ui8_pRes, uint16 *ui16_pResLen)

Description: 	此函数用于SID_RC_SUBID_STR服务回调

Parameters: 	ui8_pInd(不含SID和SubID)
				ui8_pRes(不含SID和SubID)
				ui16_pResLen

Return:       	boolean

Return Values:  E_OK		任务完成
				E_NOT_OK	任务未完成

Remarks:

Example:
*/
extern	boolean UdsFunc_SID_RC_SUBID_STR_Callback
	(uint8 *ui8_pInd, uint8 *ui8_pRes, uint16 *ui16_pResLen);

