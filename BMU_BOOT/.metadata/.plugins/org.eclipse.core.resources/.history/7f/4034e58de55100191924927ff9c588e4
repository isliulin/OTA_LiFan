#define	UDS_P2EX_SERVER_MAX	5000U	/*P2*Server最大2000ms，10ms分辨率*/	/*weihong*/

#define	UDS_TASK_CYCLE_TIME	1U		/*TASK周期时间10ms*/
#define	UDS_S3_SERVER_MAX	5000U	/*S3Server最大500个TASK周期*/
