
#define	UDS_APPL_VALID_FLAG_ADDR	0x80008000U	/*APPL有效标志地址*/

#if ECU_TYPE == ECU_VCU
#define	UDS_CMD_RX_MO_EVBUS		UDS_PHYSADDRREQ_7C2_CANOBJ		/*UDS命令接收MO*/
#define UDS_CMD_RX_MO_EVBUS_FUN UDS_FUNCADDRREQ_7DF_CANOBJ
#define	UDS_CMD_TX_MO_EVBUS		UDS_PHYSADDRRESP_7CA_CANOBJ		/*UDS命令发送MO*/

#define	UDS_CAN_PHY_ADDR_EVBUS	0x7E1U				/*物理地址*/
#define	UDS_CAN_FUNC_ADDR_EVBUS	0x7DFU				/*功能地址*/
#elif ECU_TYPE == ECU_VBU

#define	UDS_CMD_RX_MO_EVBUS		UDS_PHYSADDRREQ_7C2_CANOBJ		/*UDS命令接收MO*/
#define UDS_CMD_RX_MO_EVBUS_FUN UDS_FUNCADDRREQ_7DF_CANOBJ
#define	UDS_CMD_TX_MO_EVBUS		UDS_PHYSADDRRESP_7CA_CANOBJ		/*UDS命令发送MO*/

#define	UDS_CAN_PHY_ADDR_EVBUS	0x7E1U				/*物理地址*/
#define	UDS_CAN_FUNC_ADDR_EVBUS	0x7DFU				/*功能地址*/

#elif ECU_TYPE == ECU_BMS

#define	UDS_CMD_RX_MO_INBUS		MO_1EFF000A		/*UDS命令接收MO*/
#define UDS_CMD_RX_MO_INBUS_FUN MO_1DFFFFFF
#define	UDS_CMD_TX_MO_INBUS		MO_1EFE000A		/*UDS命令发送MO*/
//wxc20180608
//#define	UDS_CMD_RX_MO_EVBUS		MO_7E1		/*UDS命令接收MO*/
#define	UDS_CMD_RX_MO_EVBUS		MO_713		/*UDS命令接收MO*/
#define UDS_CMD_RX_MO_EVBUS_FUN MO_7DF
//#define	UDS_CMD_TX_MO_EVBUS		MO_7E9		/*UDS命令发送MO*/
#define	UDS_CMD_TX_MO_EVBUS		MO_71B		/*UDS命令发送MO*/
#define	UDS_CMD_RX_MO_INBUS_EX  MO_1E1		/*UDS命令接收MO*/
#define	UDS_CMD_TX_MO_INBUS_EX	MO_1E9		/*UDS命令发送MO*/

#define	UDS_CAN_PHY_ADDR_INBUS	0x1EFF000AU				/*物理地址*/
#define	UDS_CAN_FUNC_ADDR_INBUS	0x1DFFFFFFU				/*功能地址*/
//wxc20180608
//#define	UDS_CAN_PHY_ADDR_EVBUS	0x7E1U				/*物理地址*/
#define	UDS_CAN_PHY_ADDR_EVBUS	0x781U				/*物理地址*/
#define	UDS_CAN_FUNC_ADDR_EVBUS	0x7DFU				/*功能地址*/
#define	UDS_CAN_PHY_ADDR_INBUS_EX	0x1E1U				/*物理地址*/
#define	UDS_CAN_FUNC_ADDR_INBUS_EX	0x7DFU				/*功能地址*/

#elif ECU_TYPE == ECU_MCU
#define	UDS_CMD_RX_MO_EVBUS		53 //UDS_PHYSADDRREQ_7C2_CANOBJ		/*UDS命令接收MO*/
#define UDS_CMD_RX_MO_EVBUS_FUN 55 //UDS_FUNCADDRREQ_7DF_CANOBJ
#define	UDS_CMD_TX_MO_EVBUS		54 //UDS_PHYSADDRRESP_7CA_CANOBJ		/*UDS命令发送MO*/

#define	UDS_CAN_PHY_ADDR_EVBUS	0x7E0U				/*物理地址*/
#define	UDS_CAN_FUNC_ADDR_EVBUS	0x7DFU				/*功能地址*/

#elif ECU_TYPE == ECU_BMS_FRWK_3

#define	UDS_CMD_RX_MO_INBUS		MO_1EFF000A		/*UDS命令接收MO*/
#define UDS_CMD_RX_MO_INBUS_FUN MO_1DFFFFFF
#define	UDS_CMD_TX_MO_INBUS		MO_1EFE000A		/*UDS命令发送MO*/
#define	UDS_CMD_RX_MO_EVBUS		MO_7E1		/*UDS命令接收MO*/
#define UDS_CMD_RX_MO_EVBUS_FUN MO_7DF
#define	UDS_CMD_TX_MO_EVBUS		MO_7E9		/*UDS命令发送MO*/
#define	UDS_CMD_RX_MO_INBUS_EX  MO_1E1		/*UDS命令接收MO*/
#define	UDS_CMD_TX_MO_INBUS_EX	MO_1E9		/*UDS命令发送MO*/

#define	UDS_CAN_PHY_ADDR_INBUS	0x1EFF000AU				/*物理地址*/
#define	UDS_CAN_FUNC_ADDR_INBUS	0x1DFFFFFFU				/*功能地址*/
#define	UDS_CAN_PHY_ADDR_EVBUS	0x7E1U				/*物理地址*/
#define	UDS_CAN_FUNC_ADDR_EVBUS	0x7DFU				/*功能地址*/
#define	UDS_CAN_PHY_ADDR_INBUS_EX	0x1E1U				/*物理地址*/
#define	UDS_CAN_FUNC_ADDR_INBUS_EX	0x7DFU				/*功能地址*/

#endif

#define	UDS_APPL_DL_MAX		1200U	/*UDS TP层Buf长度*/

#define	UDS_P2_SERVER_MAX	50U		/*P2Server最大50ms，1ms分辨率*/
